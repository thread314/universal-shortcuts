{
  "appvariables": {
    "unmapcommand": "",
    "mapcommand": "map <?> <!>",
    "dotfilelocation": "~/.config/ranger/rc.conf",
    "defaultmodifier": "<?>",
    "commentmarker": "#"
  },
  "tabshash": {
    "opennewtab": {
      "function": "tab_new"
    },
    "selectprevioustab": {
      "function": "tab_move -1"
    },
    "selectnexttab": {
      "function": "tab_move +1"
    },
    "shifttableft": {
      "function": "tab_shift -1"
    },
    "shifttabright": {
      "function": "tab_shift +1"
    },
    "closecurrenttab": {
      "function": "quit"
    },
    "selectfirsttab": {
      "function": "tab_open 1"
    },
    "selectlasttab": {
      "function": "tab_open -1"
    },
    "buffernext": {
      "function": "history_go +1"
    },
    "bufferprevious": {
      "function": "history_go -1"
    },
    "bufferclose": {
      "function": ""
    }
  },
  "viewshash": {
    "stepforward": {
      "function": "scroll 1"
    },
    "stepback": {
      "function": "scroll -1"
    },
    "jumpforward": {
      "function": "move down=1 pages=True"
    },
    "jumpback": {
      "function": "move up=1 pages=True"
    },
    "find": {
      "function": "console search%space"
    },
    "gotostart": {
      "function": "move to=0"
    },
    "gotoend": {
      "function": "move to=-1"
    },
    "zoomin": {
      "function": ""
    },
    "zoomout": {
      "function": ""
    },
    "resetzoom": {
      "function": ""
    }
  },
  "fileshash": {
    "openfile": {
      "function": "open_with"
    },
    "newfile": {
      "function": "console touch%space"
    },
    "savefile": {
      "function": ""
    },
    "saveandclose": {
      "function": "q!"
    },
    "closenosave": {
      "function": "q!"
    },
    "savefileas": {
      "function": ""
    },
    "searchforfile": {
      "function": ""
    },
    "savesession": {
      "function": ""
    },
    "loadsession": {
      "function": ""
    }
  }
}
