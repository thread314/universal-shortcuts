    "mapcommand": "map <?> <!>",
    "dotfilelocation": "~/.config/kitty/kitty.conf",
    "defaultmodifier": "ctrl+shift+<?>",
    "commentmarker": "#"
  },
  "tabshash": {
    "opennewtab": {
      "function": "new_tab_with_cwd"
    },
    "selectprevioustab": {
      "function": "previous_tab"
    },
    "selectnexttab": {
      "function": "next_tab"
    },
    "shifttableft": {
      "function": "move_tab_backward"
    },
    "shifttabright": {
      "function": "move_tab_forward"
    },
    "closecurrenttab": {
      "function": "close_tab"
    },
